@inproceedings{ ISI:000166608700012,
Author = {Licht, G and Zoz, K},
Editor = {{Encaoua, D and Hall, BH and Laisney, F and Mairesse, J}},
Title = {{Patents and R\&D - An econometric investigation using applications for
   German, European and US patents by German companies}},
Booktitle = {{ECONOMICS AND ECONOMETRICS OF INNOVATION}},
Year = {{2000}},
Pages = {{307-338}},
Note = {{10th International Conference of the
   Army-Data-Retrieval-Engineering-System, STRASBOURG, FRANCE, JUN 03-05,
   1996}},
