@article{ ISI:000401395800002,
Author = {Burton, M. Diane and Nicholas, Tom},
Title = {{Prizes, patents and the search for longitude}},
Journal = {{EXPLORATIONS IN ECONOMIC HISTORY}},
Year = {{2017}},
Volume = {{64}},
Pages = {{21-36}},
Month = {{APR}},
Abstract = {{The 1714 Longitude Act created the Board of Longitude to administer a
   large monetary prize and progress payments for the precise determination
   of a ship's longitude. However, the prize did not prohibit patenting. We
   use a new dataset of marine chronometer inventors to show that the
   propensity to patent was high. We argue that while the prize spurred
   entry by key inventors, and progress payments facilitated research
   investment in an area of significant social value, patents promoted
   disclosure. Our findings highlight the importance of complementarities
   between prize and patent-based incentives in the design of innovation
   inducement contests.}},
