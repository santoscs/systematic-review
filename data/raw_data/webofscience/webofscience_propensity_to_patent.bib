@article{ ISI:000287772000013,
Author = {Nicholas, Tom},
Title = {{Cheaper patents}},
Journal = {{RESEARCH POLICY}},
Year = {{2011}},
Volume = {{40}},
Number = {{2}},
Pages = {{325-339}},
Month = {{MAR}},
Abstract = {{The 1883 Patents Act in Britain provides perspective for modern patent
   policy reforms because it radically changed incentives for inventors by
   reducing filing fees by 84 percent. Patents increased 2.5-fold after the
   reform, which was evenly distributed across the geography of inventors,
   the organization of invention and sectors. By realizing a large demand
   for cheaper patents the reform increased the propensity to patent and
   shifted inventive activity inside the patent system. It did not increase
   innovation as measured by changes in the distribution of high and low
   value patents and citations to English inventor patents in the United
   States. (C) 2010 Elsevier B.V. All rights reserved.}},
